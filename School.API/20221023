2022-10-23 22:26:51.812 +03:00 [DBG] Executing DbCommand [Parameters=[], CommandType='"Text"', CommandTimeout='30']
SELECT [s].[StudentId], [s].[IsDeleted], [s].[Name], [t].[CourseId], [t].[Description], [t].[IsDeleted], [t].[Name], [t].[StudentId]
FROM [Students] AS [s]
LEFT JOIN (
    SELECT [c].[CourseId], [c].[Description], [c].[IsDeleted], [c].[Name], [c].[StudentId]
    FROM [Courses] AS [c]
    WHERE [c].[StudentId] = 1
) AS [t] ON [s].[StudentId] = [t].[StudentId]
WHERE [s].[IsDeleted] = CAST(0 AS bit)
ORDER BY [s].[StudentId]
2022-10-23 22:26:51.884 +03:00 [INF] Executed DbCommand (77ms) [Parameters=[], CommandType='"Text"', CommandTimeout='30']
SELECT [s].[StudentId], [s].[IsDeleted], [s].[Name], [t].[CourseId], [t].[Description], [t].[IsDeleted], [t].[Name], [t].[StudentId]
FROM [Students] AS [s]
LEFT JOIN (
    SELECT [c].[CourseId], [c].[Description], [c].[IsDeleted], [c].[Name], [c].[StudentId]
    FROM [Courses] AS [c]
    WHERE [c].[StudentId] = 1
) AS [t] ON [s].[StudentId] = [t].[StudentId]
WHERE [s].[IsDeleted] = CAST(0 AS bit)
ORDER BY [s].[StudentId]
